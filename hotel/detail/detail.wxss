page {
  background-color: #fff;
}
.backimg {
  height: 400rpx;
  width: 100%;
  overflow: hidden;
  position: relative;
  z-index: -1;
}
.backimg image {
  height: 400rpx;
  width: 100%;
  /* filter: blur(20rpx); */
}
.yuan_view {
  position: absolute;
}
.card {
  box-sizing: border-box;
  margin: -150rpx auto 40rpx auto;
  width: 94%;
  padding: 20rpx;
  background-color: #fff;
  border-radius: 10rpx;
  box-shadow: 0 10px 10px 0 #f5f5f5;
  /* box-shadow: 0 0 10rpx 0 #e1e1e1; */
}
.title {
  font-size: 36rpx;
  color: #111;
  margin-bottom: 20rpx;
}
.hotel_tag {
  color: #666;
  padding-bottom: 20rpx;
}
.hotel_tag text {
  font-size: 28rpx;
  padding-left: 10rpx;
}
.hotel_tag text:first-child {
  padding-left: 0;
}
.van_tag {
  padding-right: 20rpx;
}
.adress {
  text-align: justify;
  font-size: 32rpx;
  font-weight: 600;
  color: #232323;
  padding-bottom:  20rpx;
}
.icon_one {
  color: #999;
  font-size: 32rpx;
  padding-bottom: 30rpx;
  display: flex;
}
.icon_one .ahh_button{
  margin: 0;
  padding: 0 0 0 80rpx;
  font-size: 28rpx;
  height: 40rpx;
  width: 165rpx;
  line-height: 40rpx;
  color: #519aba;
}
.icon_one .ahh {
  margin: 0 10rpx 0 -5rpx;
}
.fwlist {
  box-sizing: border-box;
  padding: 0 3%;
}

.rztime {
  background-color: #f5f5f5;
  padding: 20rpx;
}
.fxlist_item {
  display: flex;
  margin-bottom: 20rpx;
}
.content {
  margin-left: 20rpx;
  justify-content: space-between;
}
.content h4 {
  font-size: 32rpx;
  color: #111;
  font-weight: 600;
}
.content_text {
  color: #666;
  font-size: 28rpx;
}
.money {
  color: red;
  font-size: 40rpx;
  font-weight: 600;
}
.screening {
  margin: 20rpx 0;
  display: flex;
  justify-content: space-between;
}
.scroll-view {
  width:80%;
  height:80rpx;
  white-space:nowrap;
  line-height: 80rpx;
}
.screenText {
  line-height: 80rpx;
}

.df_view {
  padding: 0 20rpx;
  margin-bottom: 40rpx;
}
.df_view h4 {
  font-weight:  600;
}
.df_view h4 .ahh {
  color: #FE4F4F;
  font-size: 36rpx;
  padding-right: 10rpx;
}
.yuan {
  background-color: #BABABA;
  width: 10rpx;
  height: 10rpx;
  border-radius: 50%;
  display: inline-block;
  margin:  0 25rpx 0 10rpx;
}
.df_view view {
  font-size: 28rpx;
  display: flex;
  align-items: center;
}

swiper {
  width: 100%;
  height: 360rpx;
}

/* 设置轮播图图片属性 */
swiper-item image {
  width: 100%;
  height: 100%;
}

.sheet_content {
  font-size: 28rpx;
  padding: 20px;
}
.sheet_title {
  font-weight: 600;
  font-size: 32rpx;
}
.sheet_icon {
  display: flex;
  flex-wrap: wrap;
  width: 100%;
  
}
.sheet_item {
  width: 33.33%;
  font-size: 28rpx;
  margin-top: 20rpx;
}
.sheet_item .ahh {
  color: #AFAFAF;
  padding-right: 10rpx;
}
.sheet_item text {
  color: #262626;
}
.fxsm {
  margin-top: 40rpx;
}
.fxsm_title {
  display: inline-block;
  font-weight: 600;
  margin-bottom: 10rpx;
  font-size: 32rpx;
}
.fxsm_content {
  font-size: 28rpx;
  color: #565656;
}
.kfbtn text{
  color: #999;
  font-weight: 600;
}
.fyzz {
  margin-top: 40rpx;
  display: flex;
  
}
.fyzz_title {
  color: #595959;
  padding-right: 40rpx;
}
.fgx {
  height: 20rpx;
  width: 100%;
  margin: 0 0 20rpx 0;
  background-color: #F4F4F4;
}
.box {
  background-color: #F4F4F4;
  border-top: 20rpx solid #F4F4F4;
}
.prompt {
  padding: 40rpx;
  border-radius: 28rpx;
  background-color: #fff;
}
.impPrompt {
  font-weight: bolder;
  font-size: 38rpx;
}
.prompt view {
  padding: 10rpx;
  padding-bottom: 20rpx;
  border-bottom: 2rpx solid #ebebeb;
}
.prompt view:last-child {
  border: none;
}
.prompt view span {
  display: block;
  font-size: 30rpx;
  font-weight: bold;
  padding-bottom: 10rpx;
}
.prompt view p {
  font-size: 28rpx;
  color: #666;
}
.bottom {
  margin-bottom: 100rpx;
}
.fymx {
  padding: 40rpx;
  font-size: 28rpx;
}
.fymx_title {
  font-size: 32rpx;
  font-weight: 600;
  color: #262626;
}
.fymx_content {
  margin-top: 20rpx;
  padding: 40rpx;
  border-radius: 10rpx;
  background-color: #F4F4F4;
}