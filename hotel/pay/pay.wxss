.bgImgBox {
  width: 100%;
  height: 80rpx;
  position: relative;
  z-index: -1;
}

.bgImgBox image {
  height: 400rpx;
  width: 100%;
}
.card {
  width: 94%;
  border-radius: 10rpx;
  box-sizing: border-box;
  margin: 0 auto 20rpx auto;
  padding: 30rpx;
  background-color: #FFF;
  font-size: 28rpx;
}

.card text {
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.one view {
  margin-bottom: 20rpx;
}
.one view:last-child {
  margin-bottom: 0;
}
.ft_title {
  font-size: 32rpx;
  color: #262626;
}
.ft_stitle{
  font-size: 28rpx;
  color: #999;
  display: flex;
  align-items: center;
  margin-top: 20rpx;
}
.fontBig{
  font-size: 50rpx !important;
}
.yuan {
  display: inline-block;
  width: 10rpx;
  height: 10rpx;
  border-radius: 50%;
  background-color: #999;
  margin: 0  20rpx 0 10rpx;
}
.icongantanhao {
  margin-right: 10rpx;
  color: #FB574B;
}
.two view{
  margin-bottom: 10rpx;
  display: flex;
  align-items: center;
}
.two view text {
  color: #999;
}
#title {
  justify-content: space-between;
}
.two_title {
  width: 71%;
  font-weight: 500;
}
.ckqb {
  flex: 1;
  color: #6871f3;
}
.iconxiangyou {
  padding-top: 5rpx;
}
.liufan {
  height: 60rpx;
  line-height: 60rpx;
}
.liufan .iconwanshang {
  color: #01B1A1;
  font-size: 32rpx;
  padding-right: 10rpx;
}
.fymx_title {
  font-weight: 600;
  color: #262626;
  font-size: 28rpx;
  padding: 30rpx 30rpx 0 30rpx;
}
.zxzf {
  padding-top: 0;
}
.price {
  color: #E95150;
}
.delpd{
  padding: 0;
}
.bottom {
  margin-bottom: 80rpx;
  height: 20rpx;
}
.zyts {
  font-weight: 600;
  color: #262626;
}
.tkgz {
  margin-bottom: 20rpx;
  color: #111;
}
.tkgz_content {
  color: #999;
  text-align: justify;
}
.popup_title {
  text-align: center;
  margin-top: 25rpx;
  height: 40rpx;
}
.popup_ts {
  display: flex;
  margin-bottom: 20rpx;
}
.popup_ts h4 {
  font-weight: 500;
  color: #000;
  width: 20%;
  font-weight: bold;
}
.popup_content {
  flex: 1;
}
.popup_view {
  padding: 0 20rpx 20rpx 20rpx;
}
.room_content {
  padding: 40rpx 20rpx;
  box-sizing: border-box;
}
.pop_tag {
  display: flex;
  flex-wrap: wrap;
}
.tag_time {
  font-size: 28rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  width: 30%;
  margin: 20rpx 30rpx 0 0;
  background-color: #eee;
}
.tag_time:nth-child(3n) {
  margin-right: 0;
}
.room_text {
  margin-top: 10rpx;
  color: #999;
  font-size: 28rpx;
}