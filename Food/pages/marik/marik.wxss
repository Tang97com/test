
.container {
  margin-bottom: 100rpx;
  padding: 40rpx 0 20rpx 0;
}
.bottom {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  background-color: #Fff;
}
.hall {
  text-align: center;
  background-image: linear-gradient(120deg, #89f7fe 0%, #66a6ff 100%);
  width: 50%;
}
.markMine {
  width: 50%;
  text-align: center;
  background-image: linear-gradient(-120deg, #89f7fe 0%, #66a6ff 100%);
}
.bottomTitle {
  text-align: center;
  font-weight: 550;
  font-size: 28rpx;
  color: #6a6a6a;
}

.screenBox{
  height: 100rpx;
  display: flex;
  font-size: 28rpx;
  font-weight: 500;
  background-color: #fff;
}
.screenView {
  text-align: center;
  width: 100%;
  border: 1rpx #82acff solid;
  padding: 10rpx 30rpx;
  margin: 0 20rpx;
  color: #999;
}
.screenView  .ahh {
  color: #82acff;
}