
.container {
  margin: 40rpx auto 140rpx auto;
  width: 95%;
}
.list {
  background-color: #fff;
  padding: 20rpx;
  margin-bottom: 10rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.itemBox {
  width: 70%;
}
.filename {
  font-size: 28rpx;
  font-weight: 500;
  color: #6a6a6a;
  padding-left: 20rpx;
}
.printPage {
  position: fixed;
  bottom: 0;
  left: 0;
  height: 120rpx;
  line-height: 120rpx;
  background-color: #2F91FF;
  text-align: center;
  width: 100%;
  color: #fff;
}
.addimg {
  margin: 20rpx 0 140rpx 0;
  background-color: #fff;
  padding: 40rpx 0;
  justify-content: center;
  display: flex;
  align-items: center;
  border: 1rpx solid #e1e1e1;
  border-radius: 20rpx;
  box-sizing: border-box;
  color: #111;
}
.addimg .ahh {
  font-weight: 500;
  font-size: 40rpx;
  margin: 0 10rpx;
  color: #2F91FF;
}
.pop {
  background-color: #fff;
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 40%;
  font-weight: 500;
  font-size: 28rpx;
  border-radius: 5rpx;
  border: 1rpx solid #e1e1e1;
  z-index: 3;
  position: fixed;
  top: 30%;
  left: 30%;
  transform: translate(-30%, -30%);
}
.pop view {
  width: 100%;
  text-align: center;
  padding: 40rpx 0;
  box-sizing: border-box;
  background-color: #4481eb;
  color: #fff;
}
.pop view:first-child {
  /* padding-bottom: 0; */
  border-bottom: 1rpx solid #f5f5f5;
}
.cotactnum {
  width: 20%;
  margin-right: 40rpx;
}

.cotactnum {
  color: #4481eb;
}