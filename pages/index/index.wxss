.location{
  position: absolute;
  color: #fff;
  font-size: 28rpx;
  width: 60%;
  display: flex;
}
.seeclot-title{
  width: 60%;
  padding-left: 10rpx;
}
.navBarTitle{
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
  color: #fff;
  font-size: 28rpx;
}

.head {
  height: 360rpx;
  width: 100%;
  background-color: burlywood;
}
.container {
  box-sizing: border-box;
}
.small {
  width: 100%;
  display: flex;
  flex-wrap: wrap;
}
.item {
  width: 33.3%;
  /* margin: 40rpx 1% 0 1%; */
  margin: 40rpx 0 0 0 ;
  text-align: center;
  /* background-color: red; */
}
.elet {
  width: 100rpx;
  height: 100rpx;
  background-color: #66a6ff;
  border-radius: 50%;
}
.images {
  width: 80rpx;
  height: 80rpx;
  padding-bottom: 10rpx;
  /* border-radius: 50%; */
}
.banner {
  height: 250rpx;
  margin: 40rpx auto;
  background-color: #66a6ff;
  width: 95%;
  border-radius: 10rpx;
  text-align: center;
  opacity: .5;
}
.view_images {
  height:  200rpx;
  text-align: center;
  margin: 5vh auto 40rpx auto;
}
.view_img {
  height: 200rpx;
}