page {
  background-color: #fff;
  width: 100%;
  height: 100%;
}
.cpts {
  padding: 20rpx;
}

.name,
.contact {
  padding: 20rpx;
  background-color: #e1e1e1;
  font-size: 28rpx;
  border-radius: 10rpx;
}

.contact {
  margin: 40rpx 0;
}

.content {
  box-sizing: border-box;
  width: 100%;
  padding: 20rpx;
  background-color: #e1e1e1;
  margin-bottom: 40rpx;
  border-radius: 10rpx;
  font-size: 28rpx;
}
.server {
  display: flex;
  align-items: center;
  font-size: 30rpx;
  padding-bottom: 20rpx;
}

.marik {
  padding-right: 20rpx;
}
.btns {
  background-color: #2F91FF;
  color: #fff;
}